                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="ORC_IZ 1_5">
            <Constraint ID="IZ-25">
                <Description>The value of ORC_IZ 1_5.1 (Order Control) SHALL be 'RE'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="RE" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="RXA_IZ 1_5">
            <Constraint ID="IZ-28">
                <Description>The value of RXA_IZ 1_5.1 (Give Sub-ID Counter) SHALL be '0'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="0" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-29">
                <Description>The value of RXA_IZ 1_5.2 (Administration Sub-ID Counter) SHALL be '1'.</Description>
                <Assertion>
                    <PlainText Path="2[1]" Text="1" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-30">
                <Description>RXA_IZ 1_5.4 (Date/Time End of Administration) SHALL be identical to the RXA_IZ 1_5.3 (Date/Time Start of Administration).</Description>
                <Assertion>
                    <PathValue Path1="4[1]" Operator="EQ" Path2="3"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-48">
                <Description>If the value of RXA_IZ 1_5.20 (Completion Status) is 'RE', then the value of RXA_IZ 1_5.6 (Administered Amount) SHALL be '999'.</Description>
                <Assertion>
                    <IMPLY>
                        <PlainText Path="20[1]" Text="RE" IgnoreCase="true"/>
                        <PlainText Path="6[1]" Text="999" IgnoreCase="true"/>
                    </IMPLY>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ_50">
                <Description>If the value of RXA_IZ 1_5.9[1].1 (Identifier) is not '00', then the value of RXA_IZ 1_5.6 (Administered Amount) SHALL be '999'.</Description>
                <Assertion>
                    <IMPLY>
                        <NOT>
                            <PlainText Path="9[1].1[1]" Text="00" IgnoreCase="true"/>
                        </NOT>
                        <PlainText Path="6[1]" Text="999" IgnoreCase="true"/>
                    </IMPLY>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-49">
                <Description>If the value of RXA_IZ 1_5.1 (Give Sub-ID Counter) is '998', then the value of RXA_IZ 1_5.6 (Administered Amount) SHALL be '999'.</Description>
                <Assertion>
                    <IMPLY>
                        <PlainText Path="1[1]" Text="998" IgnoreCase="true"/>
                        <PlainText Path="6[1]" Text="999" IgnoreCase="true"/>
                    </IMPLY>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-32">
                <Description>If RXA_IZ 1_5.18[1] (Substance/Treatment Refusal Reason) is valued, then the value of RXA_IZ 1.5.20 (Completion Status) SHALL be 'RE'.</Description>
                <Assertion>
                    <IMPLY>
                        <Presence Path="18[1]"/>
                        <PlainText Path="20[1]" Text="RE" IgnoreCase="true"/>
                    </IMPLY>
                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="OBX_IZ 1_5">
